{
  "expo": {
    "name": "FairEnough",
    "slug": "fairenough",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/bluespiral.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spiralsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F2ED"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "005b2f3f-3284-439f-ba29-c85599c06085"
      }
    },
    "android": {
      "package": "com.srconstantin.fairenough"
    },
    "ios": {
      "bundleIdentifier": "com.srconstantin.fairenough",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
    "web": {
      "favicon": "./assets/bluespiral.png",
      "bundler": "metro"
      }
    }
  }
}